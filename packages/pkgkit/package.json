{
  "name": "@fengshangwuqi/pkgkit",
  "version": "1.0.37",
  "author": "fengshangwuqi <fengshangwuqi@gmail.com>",
  "bin": {
    "pkgkit": "bin/pkgkit.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "add": "ts-node bin/pkgkit.js add",
    "build": "ts-node bin/pkgkit.js build",
    "tsc": "tsc --emitDeclarationOnly --diagnostics false --outDir ../../typings",
    "prepare": "yarn clean && yarn tsc && cross-env NODE_ENV=production yarn build"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/core": "7.12.3",
    "@fengshangwuqi/babel-preset": "1.0.15",
    "@rollup/plugin-babel": "5.2.1",
    "@rollup/plugin-commonjs": "16.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "10.0.0",
    "chalk": "4.1.0",
    "execa": "4.1.0",
    "fs-extra": "9.0.1",
    "rollup": "2.32.1",
    "rollup-plugin-dts": "1.4.13",
    "rollup-plugin-node-builtins": "2.1.2",
    "typescript": "4.0.5",
    "yargs": "16.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.2",
    "@types/rollup-plugin-node-builtins": "2.1.1",
    "@types/yargs": "15.0.9"
  },
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js"
}
